{
    "name": "__MSG_ext_name__",
    "version": "0.1.3",
    "description": "__MSG_ext_description__",
    "default_locale": "en",
    "permissions": [
        "storage",
        "{{HOOK_URL}}"
    ],
    "content_scripts": [
    {
        "matches": ["{{SITE_TO_TEST}}"],
        "js": ["content.js"]
    }],
    "background":
    {
        "scripts": [
            "background-env.js", "background.js"
        ]
    },
    "browser_action":
    {
        "default_popup": "popup.html",
        "default_icon":
        {
            "16": "images/logo16.png",
            "32": "images/logo32.png",
            "48": "images/logo48.png",
            "128": "images/logo128.png"
        }
    },
    "icons":
    {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    },
    "manifest_version": 2
}